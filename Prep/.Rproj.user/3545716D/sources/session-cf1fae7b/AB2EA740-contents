#Calculation for beach seine volume
beachseine_crew$volume<-((0.5*beachseine_crew$DepthMeters*beachseine_crew$LengthOfTrawl*beachseine_crew$Width))

#check that volumes calculated
table(beachseine_crew['volume'])

